{
    "manifest_version": 2,
    "name": "VIX",
    "description": "VIX is a browser extension that adds WCAG and ARIA tags to web pages, and summarizes the page content.",
    "homepage_url": "https://github.com/gabrielmoterani/VIX-monorepo",
    "author": "TL Studio & Algoma University",
    "version": "1.0",
    "icons": {
      "48": "icons/icon.png",
      "96": "icons/icon@2x.png"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["./src/domToJson.js", "./src/substitute.js", "./src/pageHandler.js"]
      }
    ],
    "permissions": [
      "webRequest", 
      "https://11uhx8j6hf.execute-api.us-east-1.amazonaws.com/*"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
}
